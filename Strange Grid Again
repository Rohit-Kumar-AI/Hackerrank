#include <stdio.h>
#include <stdlib.h>

long long strangeGrid(int r, int c) {
    long long row = r; 
    long long base = ((row - 1) / 2) * 10;
    if (r % 2 == 1) {
        return base + 2 * (c - 1);
    } else {
        return base + 2 * (c - 1) + 1;
    }
}

int main() {
    int r, c;
    scanf("%d %d", &r, &c);
    printf("%lld\n", strangeGrid(r, c));
    return 0;
}
