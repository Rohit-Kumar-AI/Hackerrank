#include <bits/stdc++.h>
using namespace std;

int main() {
    int n1, n2, n3;
    cin >> n1 >> n2 >> n3;

    vector<int> h1(n1), h2(n2), h3(n3);
    for (int &x : h1) cin >> x;
    for (int &x : h2) cin >> x;
    for (int &x : h3) cin >> x;

    reverse(h1.begin(), h1.end());
    reverse(h2.begin(), h2.end());
    reverse(h3.begin(), h3.end());

    vector<int> s1(n1), s2(n2), s3(n3);
    if (n1) s1[0] = h1[0];
    if (n2) s2[0] = h2[0];
    if (n3) s3[0] = h3[0];

    for (int i = 1; i < n1; i++) s1[i] = s1[i-1] + h1[i];
    for (int i = 1; i < n2; i++) s2[i] = s2[i-1] + h2[i];
    for (int i = 1; i < n3; i++) s3[i] = s3[i-1] + h3[i];

    int i = n1 - 1, j = n2 - 1, k = n3 - 1;

    while (i >= 0 && j >= 0 && k >= 0) {
        int a = s1[i], b = s2[j], c = s3[k];
        if (a == b && b == c) {
            cout << a << endl;
            return 0;
        }
        if (a >= b && a >= c) i--;
        else if (b >= a && b >= c) j--;
        else k--;
    }

    cout << 0 << endl;
    return 0;
}
