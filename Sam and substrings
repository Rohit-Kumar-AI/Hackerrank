#include <bits/stdc++.h>
using namespace std;

int main() {
    const long long MOD = 1000000007;
    string s;
    cin >> s;
    long long n = s.size();

    long long result = 0, f = 0;

    for (int i = 0; i < n; i++) {
        int num = s[i] - '0';
        f = (f * 10 + (long long)(i + 1) * num) % MOD;
        result = (result + f) % MOD;
    }

    cout << result % MOD;
    return 0;
}
