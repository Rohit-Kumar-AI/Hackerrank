#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int T;
    cin >> T;
    while (T--) {
        int N, M;
        cin >> N >> M;
        vector<long long> freq(M, 0);
        long long sum = 0, res = 0;
        freq[0] = 1;
        for (int i = 0; i < N; i++) {
            long long x;
            cin >> x;
            sum = (sum + x) % M;
            if (sum < 0) sum += M;
            res += freq[sum];
            freq[sum]++;
        }
        cout << res << "\n";
    }
    return 0;
}
